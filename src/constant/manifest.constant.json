  "schemaSpace": "http://schema.org/",
  "defaultTypes": [
    {
      "id": "schema:Event",
      "name": "Event"
    },
    {
      "id": "schema:Person",
      "name": "Person"
    },
    {
      "id": "schema:Place",
      "name": "Place"
    },
    {
      "id": "dbo:Agent",
      "name": "Agent"
    },
    {
      "id": "schema:Organization",
      "name": "Organization"
    },
    {
      "id": "skos:Concept",
      "name": "Concept"
    },
    {
      "id": "ado:EventType",
      "name": "Artsdata Event Type"
