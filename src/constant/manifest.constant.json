        "helpText": "Content type: ID or literal",
        "choices": [
          {
            "value": "id",
            "name": "ID"
          },
          {
            "value": "literal",
            "name": "Literal"
          }
        ]
      }
    ],
    "propose_properties": {
      "service_url": "https://staging.recon.artsdata.ca",
      "service_path": "/extend/propose"

    }
  },
  "view": {
    "url": "http://kg.artsdata.ca/resource/{{id}}"
  },
  "preview": {
    "height": 100,
    "width": 400,
    "url": "https://staging.recon.artsdata.ca/preview?id={{id}}"
